where ss_item_sk = i_item_sk and
      ss_sold_date_sk = d_date_sk and
      ss_store_sk = s_store_sk and
      d_year in (1999) and
        ((i_category in ('Women','Jewelry','Electronics') and
          i_class in ('swimwear','rings','dvd/vcr players')
         )
      or (i_category in ('Children','Home','Sports') and
          i_class in ('newborn','accent','football') 
        ))
