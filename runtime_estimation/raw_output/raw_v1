       c_birth_month in (10,6,11,12,3,4) and
       d_year = 2000 and
       ca_state in ('KY','TX','MN'
                   ,'TN','OH','MO','WY')
 group by rollup (i_item_id, ca_country, ca_state, ca_county)
 order by ca_country,
        ca_state, 
